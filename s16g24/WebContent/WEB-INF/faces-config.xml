		<display-name>Logout</display-name>
		<from-view-id>/PostLogin.jsp</from-view-id>
		<navigation-case><from-outcome>LOGOUT</from-outcome>
			<to-view-id>/logout.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
